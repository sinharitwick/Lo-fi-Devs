<%- include('partials/header') -%>


    <form action="/posts/<%=jobpost._id%>?_method=PUT" method="POST">
        <div class="field">
            
            <input type="text" name="jobpost[domain]" placeholder="Title" value="<%=jobpost.domain%>">
        </div>

        <div>
            
            <input type="number" name="jobpost[vacancy]" placeholder="image" value="<%=jobpost.vacancy%>">
        </div>

        <div>
            
            <textarea name="jobpost[skillreq]" cols="30" rows="10"><%=jobpost.skillreq%></textarea>
        </div>
        <input type="Submit">
    </form>
</div>

<%- include('partials/footer') -%>